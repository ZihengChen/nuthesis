@article{BANNER1983476,
title = {Observation of single isolated electrons of high transverse momentum in events with missing transverse energy at the CERN pp collider},
journal = {Physics Letters B},
volume = {122},
number = {5},
pages = {476-485},
year = {1983},
issn = {0370-2693},
doi = {https://doi.org/10.1016/0370-2693(83)91605-2},
url = {https://www.sciencedirect.com/science/article/pii/0370269383916052},
author = {M. Banner and R. Battiston and Ph. Bloch and F. Bonaudi and K. Borer and M. Borghini and J.-C. Chollet and A.G. Clark and C. Conta and P. Darriulat and L. {Di Lella} and J. Dines-Hansen and P.-A. Dorsaz and L. Fayard and M. Fraternali and D. Froidevaux and J.-M. Gaillard and O. Gildemeister and V.G. Goggi and H. Grote and B. Hahn and H. Hänni and J.R. Hansen and P. Hansen and T. Himel and V. Hungerbühler and P. Jenni and O. Kofoed-Hansen and E. Lançon and M. Livan and S. Loucatos and B. Madsen and P. Mani and B. Mansoulie and G.C. Mantovani and L. Mapelli and B. Merkel and M. Mermikides and R. Møllerud and B. Nilsson and C. Onions and G. Parrour and F. Pastore and H. Plothow-Besch and M. Polverel and J.-P. Repellin and A. Rothenberg and A. Roussarie and G. Sauvage and J. Schacher and J.L. Siegrist and H.M. Steiner and G. Stimpfl and F. Stocker and J. Teiger and V. Vercesi and A. Weidberg and H. Zaccone and W. Zeller},
abstract = {We report the results of a search for single isolated electrons of high transverse momentum at the CERN pp collider. Above 15 GeV/c, four events are found having large missing transverse energy along a direction opposite in azimuth to that of the high-pT electron. Both the configuration of the events and their number are consistent with the expectations from the process p+p→W±+anything, with W→e+ν, where W± is the charged Intermediate Vector Boso postulated by the unified electroweak theory.}
}